<?xml version='1.0' encoding='utf-8'?>
<interface>
  <object class="ttk.Frame" id="mainwindow">
    <property name="height">200</property>
    <property name="width">200</property>
    <bind add="" handler="" sequence="" />
    <layout>
      <property name="column">0</property>
      <property name="propagate">True</property>
      <property name="row">0</property>
      <property name="sticky">se</property>
      <rows>
        <row id="0">
          <property name="minsize">100</property>
          <property name="pad">10</property>
        </row>
        <row id="1">
          <property name="minsize">50</property>
          <property name="pad">10</property>
        </row>
        <row id="2">
          <property name="minsize">0</property>
        </row>
        <row id="3">
          <property name="minsize">50</property>
          <property name="pad">20</property>
        </row>
        <row id="4">
          <property name="minsize">50</property>
          <property name="pad">20</property>
        </row>
      </rows>
      <columns>
        <column id="0">
          <property name="minsize">500</property>
          <property name="pad">50</property>
        </column>
      </columns>
    </layout>
    <child>
      <object class="pygubu.builder.widgets.pathchooserinput" id="file_path">
        <property name="type">file</property>
        <bind add="" handler="on_file_change" sequence="&lt;&lt;PathChooserPathChanged&gt;&gt;" />
        <layout>
          <property name="column">0</property>
          <property name="propagate">True</property>
          <property name="row">2</property>
          <property name="sticky">w</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="pygubu.builder.widgets.pathchooserinput" id="img_path">
        <property name="type">file</property>
        <bind add="" handler="on_image_change" sequence="&lt;&lt;PathChooserPathChanged&gt;&gt;" />
        <layout>
          <property name="column">0</property>
          <property name="propagate">True</property>
          <property name="row">1</property>
          <property name="sticky">sw</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Label" id="lbl_image">
        <property name="image">noImage.gif</property>
        <property name="text" translatable="yes">Image file</property>
        <layout>
          <property name="column">0</property>
          <property name="propagate">True</property>
          <property name="row">0</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Button" id="btn_process">
        <property name="command">on_process_clicked</property>
        <property name="text" translatable="yes">Encode</property>
        <layout>
          <property name="column">0</property>
          <property name="propagate">True</property>
          <property name="row">3</property>
          <property name="sticky">w</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="tk.Text" id="txt_output">
        <property name="background">#c0c0c0</property>
        <property name="height">10</property>
        <property name="text" translatable="yes">...</property>
        <property name="width">50</property>
        <layout>
          <property name="column">0</property>
          <property name="propagate">True</property>
          <property name="row">4</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Label" id="Label_1">
        <property name="justify">left</property>
        <property name="text" translatable="yes">File Embed</property>
        <property name="width">27</property>
        <layout>
          <property name="column">0</property>
          <property name="propagate">True</property>
          <property name="row">2</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Label" id="Label_2">
        <property name="anchor">w</property>
        <property name="font">TkTextFont</property>
        <property name="justify">left</property>
        <property name="text" translatable="yes">Image File</property>
        <property name="width">27</property>
        <layout>
          <property name="column">0</property>
          <property name="propagate">True</property>
          <property name="row">1</property>
          <property name="sticky">s</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Entry" id="txt_alpha">
        <property name="text" translatable="yes">Init alpha value</property>
        <property name="textvariable">string:var_alpha</property>
        <layout>
          <property name="column">0</property>
          <property name="propagate">True</property>
          <property name="row">1</property>
          <property name="sticky">nw</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Button" id="btn_capacity">
        <property name="command">on_capacity_clicked</property>
        <property name="text" translatable="yes">Capacity</property>
        <property name="width">10</property>
        <layout>
          <property name="padx">180</property>
          <property name="propagate">True</property>
          <property name="row">3</property>
          <property name="sticky">w</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Button" id="btn_decode">
        <property name="command">on_decode_clicked</property>
        <property name="text" translatable="yes">Decode</property>
        <layout>
          <property name="column">0</property>
          <property name="padx">90</property>
          <property name="propagate">True</property>
          <property name="row">3</property>
          <property name="sticky">w</property>
        </layout>
      </object>
    </child>
  </object>
</interface>
